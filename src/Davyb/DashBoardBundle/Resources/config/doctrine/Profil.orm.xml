<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Davyb\DashBoardBundle\Entity\Profil" table="profil">
    <indexes>
      <index name="fk_user_idx" columns="id_user"/>
      <index name="fk_sexe_idx" columns="id_sexe"/>
    </indexes>
    <id name="idProfil" type="integer" column="id_profil">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="45" nullable="true"/>
    <field name="prenom" type="string" column="prenom" length="45" nullable="true"/>
    <field name="pseudo" type="string" column="pseudo" length="45" nullable="true"/>
    <field name="dateNaissance" type="string" column="date_naissance" length="45" nullable="true"/>
    <field name="cheminPhotoProfil" type="string" column="chemin_photo_profil" length="256" nullable="true"/>
    <field name="description" type="string" column="description" length="512" nullable="true"/>
    <many-to-one field="idUser" target-entity="User">
      <join-columns>
        <join-column name="id_user" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idSexe" target-entity="Sexe">
      <join-columns>
        <join-column name="id_sexe" referenced-column-name="id_sexe"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
